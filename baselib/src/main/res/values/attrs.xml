<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MsgView">
        <!-- 圆角矩形背景色 -->
        <attr name="mv_backgroundColor" format="color"/>
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_cornerRadius" format="dimension"/>
        <!-- 圆角弧度,单位dp-->
        <attr name="mv_strokeWidth" format="dimension"/>
        <!-- 圆角边框颜色-->
        <attr name="mv_strokeColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="TabLayout">

    </declare-styleable>


    <declare-styleable name="ProgressView">
        <attr name="progress_background_color" format="color"/>
        <attr name="progress_progress_color" format="color"/>
        <attr name="progress_progress_tv_color" format="color"/>
        <attr name="progress_progress_tv_size" format="dimension"/>
        <attr name="progress_progress_number" format="integer"/>
        <attr name="progress_real" format="boolean"/>
        <attr name="progress_show_tv" format="boolean"/>


        <attr name="progress_thumb_width" format="dimension"/>
        <attr name="progress_thumb_height" format="dimension"/>
        <attr name="progress_thumb_radius" format="dimension"/>
        <attr name="progress_thumb_color" format="color"/>
        <attr name="progress_thumb_shadow_color" format="color"/>
        <attr name="progress_thumb_shadow_radius" format="dimension"/>
        <attr name="progress_min_height" format="dimension"/>
        <attr name="progress_max_height" format="dimension"/>

        <attr name="progress_height" format="dimension"/>
        <attr name="progress_thumb_min_size" format="dimension"/>
        <attr name="progress_thumb_max_size" format="dimension"/>


        <attr name="progress_progress_circle_stoke_width" format="dimension"/>
    </declare-styleable>
</resources>