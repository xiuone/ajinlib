<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeLayout">
        <attr format="enum" name="preview">
            <enum name="none" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
            <enum name="start" value="3"/>
            <enum name="end" value="4"/>
        </attr>
        <attr format="boolean" name="autoClose"/>
        <attr format="string" name="designer"/>
    </declare-styleable>
    <declare-styleable name="SwipeMenuRecyclerView">
        <attr format="boolean" name="enableTouchAlways"/>
    </declare-styleable>
</resources>