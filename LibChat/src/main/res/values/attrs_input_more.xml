<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ChatInputMoreView">
<!--        最大行-->
        <attr name="input_more_max_line" format="integer"/>
<!--        列最小间距-->
        <attr name="input_more_min_cow_space" format="dimension"/>
<!--        行间距-->
        <attr name="input_more_line_space" format="dimension"/>
<!--        item的宽度-->
        <attr name="input_more_item_width" format="dimension"/>
<!--        item的高度-->
        <attr name="input_more_item_height" format="dimension"/>
<!--        图片大小-->
        <attr name="input_more_item_icon_size" format="dimension"/>
<!--        文字大小-->
        <attr name="input_more_item_text_size" format="dimension"/>
<!--        文字颜色-->
        <attr name="input_more_item_text_color" format="color"/>
<!--        页面上下间距-->
        <attr name="input_more_padding_top_bottom" format="dimension"/>
    </declare-styleable>
</resources>